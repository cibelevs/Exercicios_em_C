//Escreva um programa em C que leia dois números inteiros e exiba o maior deles. 
//Caso sejam iguais, exiba uma mensagem informando que os números são iguais.
#include<stdio.h>
int main()
{
    int num1, num2;
    int menor=0;
    int maior = 0;
    
    printf("Insira um número: ");
    scanf ("%d", &num1);
    
    printf("Insira um número: ");
    scanf ("%d", &num2);
    
    if (num1 == num2)
    {
        printf("Números iguais");
        return 0;
    }
    else 
        if (num1 > num2)
        {
            maior = num1;
            menor = num2;
        }
        else
        {
            maior = num2;
            menor= num1;
        }

    printf("Maior: %d \n Menor: %d \n", maior, menor);
    
    
    return 0;
}
